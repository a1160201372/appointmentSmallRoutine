<!--miniprogram/pages/certification/realname/nameNum/index.wxml-->

<view class='table'>
  <text style="color:#02C4FF"> 填写身份信息 </text>
  <text style="color:#646464"> ---> </text>
  <text style="color:#646464"> 人脸验证 </text>
  <text style="color:#646464"> ---> </text>
  <text style="color:#646464"> 验证成功 </text>
</view>


<text>\n</text>
<view class='pick'>
  <view  style="width: 200rpx;height:120rpx" class='text'>
